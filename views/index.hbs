<div class="container wrapper">
  {{#each reviews}}
  <a href="/reviews/{{this._id}}">
    <div class="item">
      <div class="top">
        <div class="title">{{this.title}}</div>
      </div>
      <div class="content">{{this.body}}</div>
      <div class="bottom">
        <div class="rating">
          <span>Rating:</span>{{this.rating}}/10
        </div>
      </div>
    </div>
  </a>
  {{/each}}
</div>
